{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "endOfLine": "lf",
        "tabWidth": 2,
        "singleQuote": false
      }
    }
  ],
  "plugins": ["prettier-plugin-solidity"]
}
